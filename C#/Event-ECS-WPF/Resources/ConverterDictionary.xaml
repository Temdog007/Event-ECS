<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:converter="clr-namespace:Event_ECS_WPF.Converters">

    <converter:BoolToVisibilityConverter x:Key="boolVis"/>
    <converter:EnumToBoolConverter x:Key="enumConv"/>
    <converter:NullCheckVisibilityConverter x:Key="nullCheck"/>
    <converter:LogLevelToColor x:Key="logToColor"/>
    <converter:ItemToItemIndexConverter x:Key="itemListConv"/>
    <converter:ColorIndexToNameConverter x:Key="colorIndexConv"/>
    <converter:ColorIndexToBackgroundConverter x:Key="colorBackgroundConv"/>
    <converter:ColorIndexToForegroundConverter x:Key="colorForegroundConv"/>
    <converter:ColorsCombinedConverted x:Key="colorComboConv"/>
    <converter:RelativeSizeConverter x:Key="relativeConv"/>
</ResourceDictionary>